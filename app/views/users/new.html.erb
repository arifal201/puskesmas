<h1>Sign Up</h1>

<%= form_for @user do |f| %>
    <%= f.label :username %><br>
    <%= f.text_field :username %><br>
    <%= f.label :password %><br>
    <%= f.text_field :password %><br>
    <%= f.label :nik %><br>
    <%= f.text_field :nik %><br>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <%= f.label :alamat %><br>
    <%= f.text_field :alamat %><br>
    <%= f.submit %>
<% end %>